{
	"schemaVersion": 1,
	"id": "kebbler",
	"version": "${version}",
	"name": "Kebbler",
	"description": "Kebbler is an API that aims to make tree, biome, and world generation more simple.",
	"authors": [
		"KeebleKapa"
	],
	"contact": {
		"homepage": "https://modrinth.com/user/keeblekapa",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/kebbler/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.keeblekapa.kebbler.api.main.Kebbler"
		],
		"fabric-datagen": [
			"net.keeblekapa.kebbler.api.main.KebblerDataGenerator"
		]
	},
	"mixins": [
		"kebbler.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}